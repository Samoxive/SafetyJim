{
    "extends": "tslint:recommended",
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "prefer-const": false,
        "ordered-imports": [
            false
        ],
        "no-var-requires": false,
        "interface-name": [
            false
        ],
        "object-literal-sort-keys": false,
        "interface-over-type-literal": false,
        "radix": false,
        "switch-default": false,
        "no-unsafe-finally": false
    }
}